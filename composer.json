{
    "name": "detailnet/denner-client",
    "description": "API Client for Denner Portal 2.0 Web Services",
    "type": "library",
    "keywords": [
        "denner",
        "detailnet",
        "api-client"
    ],
    "homepage": "https://github.com/detailnet/denner-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Wolf",
            "email": "ivan.wolf@detailnet.ch"
        },
        {
            "name": "Curzio Della Santa",
            "email": "curzio.della.santa@detailnet.ch"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:detailnet/denner-common.git"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "~5.0",
        "guzzlehttp/guzzle-services": "~0.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4.5",
        "squizlabs/php_codesniffer" : "~2.2.0",
        "phpmd/phpmd": "~2.1.3",
        "satooshi/php-coveralls": "dev-master",
        "ramsey/uuid": "~2.8",
        "detailnet/denner-common": "~0.5.1"
    },
    "suggest": {
        "ramsey/uuid": "For usage of the denner model response classes",
        "detailnet/denner-common": "For usage of the denner model response classes"
    },
    "autoload": {
        "psr-0": {
            "Denner\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "DennerTest\\Client\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "archive": {
        "exclude": [
            "!vendor",
            "tests",
            "*phpunit.xml",
            ".coveralls.yml",
            ".travis.yml",
            "build.xml",
            "build.properties",
            "composer.phar"
        ]
    }
}
